<% if (identity) { %>
<div class="weui-cells">
    <a class="weui-cell weui-cell_access" href="/user/shopqrcode?shopcode=<%= shopcode %>">
        <div class="weui-cell__bd">
            <p style="font-size: 15px">店铺推广码</p>
        </div>
        <div class="weui-cell__ft">
        </div>
    </a>
    <a class="weui-cell weui-cell_access" href="/order/orderlist?shopcode=<%= shopcode %>&referrer=<%= reguser.uid %>">
        <div class="weui-cell__bd">
            <p style="font-size: 15px">我推广的订单</p>
        </div>
        <div class="weui-cell__ft">
        </div>
    </a>
    <a class="weui-cell weui-cell_access" href="/shop/<%= shopcode %>/propertyManage">
        <div class="weui-cell__bd">
            <p style="font-size: 15px">我的收益</p>
        </div>
        <div class="weui-cell__ft">
        </div>
    </a>
    <a class="weui-cell weui-cell_access" href="/shop/<%= shopcode %>/productManage">
        <div class="weui-cell__bd">
            <p style="font-size: 15px">我的商品</p>
        </div>
        <div class="weui-cell__ft">
        </div>
    </a>
</div>

<% } %>

<% if ( userManageShops.length>0 ){ %>
<div class="weui-cells__title">
    店铺管理
</div>
<div class="weui-cells">
    <% userManageShops.forEach(function (item) { %>
    <a class="weui-cell weui-cell_access" href="/shop/<%= item.code %>/shopManage">
        <div class="weui-cell__bd">
            <p style="font-size: 15px"><%= item.name %></p>
        </div>
        <div class="weui-cell__ft">
        </div>
    </a>
    <% }) %>
</div>
<% } %>

<div class="weui-cells">
    <a class="weui-cell weui-cell_access" href="/shop/<%= shopcode %>">
        <div class="weui-cell__bd">
            <p style="font-size: 15px">商城首页</p>
        </div>
        <div class="weui-cell__ft">
        </div>
    </a>
</div>

